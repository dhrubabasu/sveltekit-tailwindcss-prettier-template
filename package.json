    "httpie": "2.0.0-next.13",
    "node-html-parser": "3.3.3",
    "polka": "0.5.2",
    "postcss": "8.2.15",
    "postcss-cli": "8.3.1",
    "prettier": "2.3.0",
    "prettier-plugin-svelte": "2.3.0",
    "rustywind": "0.8.1",
    "sirv": "1.0.11",
    "svelte": "3.38.2",
